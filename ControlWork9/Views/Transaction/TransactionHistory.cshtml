@model List<Transaction>

<h3>Журнал транзакций</h3>

<table>
    <thead>
    <tr>
        <th>Дата</th>
        <th>Сумма</th>
        <th>Тип</th>
        <th>Получатель/Отправитель</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var transaction in Model)
    {
        <tr>
            <td>@transaction.Date</td>
            <td>@transaction.Amount</td>
            <td>@transaction.Type</td>
            <td>@transaction.User</td>
        </tr>
    }
    </tbody>
</table>